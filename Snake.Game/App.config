<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--Game context-->
    <add key="width" value="135"/>
    <add key="height" value="47"/>
    <add key="playWidth" value="90"/>

    <!--Snake-->
    <add key="lifes" value="3"/>
    <add key="pointsToAdditionalLife" value="3000"/>
    <add key="initialSnakeLength" value="5"/>
    <add key="maxSnakeLength" value="17"/>
    <add key="timeGetTimeBonus" value="8"/>
    <add key="timeToBomb" value="4"/>
    <add key="bombRange" value="5"/>
    <add key="immortalMovePeriod" value="7"/>

    <!--Wall-->
    <add key="maxLevel" value="10"/>
    <add key="wallSpreadDencity" value="5"/>
    <add key="minWallLength" value="6"/>
    <add key="maxWallLength" value="17"/>
    
    <!--Food-->
    <add key="timeFoodBeEaten" value="17"/>
    
    <!--Bonus-->
    <add key="bonusPossibility" value="5"/>
    <add key="remainingTime" value="10"/>
  
  </appSettings>
</configuration>